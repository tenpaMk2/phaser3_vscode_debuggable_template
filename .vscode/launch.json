{
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://127.0.0.1:5500",
      "webRoot": "${workspaceFolder}/dist/",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack://ts/*": "${webRoot}/../*"
      }
    }
  ]
}
